        <div id="history-box" class="card p-6 hidden">
            <h2 class="font-semibold mb-4 text-gray-700">üìÇ ‡∏õ‡∏£‡∏∞‡∏ß‡∏±‡∏ï‡∏¥‡πÅ‡∏•‡∏∞‡∏™‡∏£‡∏∏‡∏õ‡∏£‡∏≤‡∏¢‡πÄ‡∏î‡∏∑‡∏≠‡∏ô</h2>
            <div id="history-content" class="space-y-4"></div>
        </div>
    </div>

    <script>
        const MIN_UNIT_PRICE = 3.99;
        let historyData = [];
        let isMonitoring = false;
        let timer = null;

        const tips = [
            "‡∏õ‡∏¥‡∏î‡πÑ‡∏ü‡∏î‡∏ß‡∏á‡∏ó‡∏µ‡πà‡πÑ‡∏°‡πà‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏õ‡∏£‡∏∞‡∏´‡∏¢‡∏±‡∏î‡∏û‡∏•‡∏±‡∏á‡∏á‡∏≤‡∏ô",
            "‡∏ï‡∏±‡πâ‡∏á‡∏≠‡∏∏‡∏ì‡∏´‡∏†‡∏π‡∏°‡∏¥‡πÅ‡∏≠‡∏£‡πå‡∏ó‡∏µ‡πà 25-26 ‡∏≠‡∏á‡∏®‡∏≤‡πÄ‡∏ã‡∏•‡πÄ‡∏ã‡∏µ‡∏¢‡∏™",
            "‡∏ñ‡∏≠‡∏î‡∏õ‡∏•‡∏±‡πä‡∏Å‡πÄ‡∏Ñ‡∏£‡∏∑‡πà‡∏≠‡∏á‡πÉ‡∏ä‡πâ‡πÑ‡∏ü‡∏ü‡πâ‡∏≤‡∏´‡∏•‡∏±‡∏á‡πÄ‡∏•‡∏¥‡∏Å‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô‡πÄ‡∏™‡∏°‡∏≠",
            "‡∏•‡πâ‡∏≤‡∏á‡πÄ‡∏Ñ‡∏£‡∏∑‡πà‡∏≠‡∏á‡∏õ‡∏£‡∏±‡∏ö‡∏≠‡∏≤‡∏Å‡∏≤‡∏®‡∏ó‡∏∏‡∏Å 6 ‡πÄ‡∏î‡∏∑‡∏≠‡∏ô‡∏ä‡πà‡∏ß‡∏¢‡∏õ‡∏£‡∏∞‡∏´‡∏¢‡∏±‡∏î‡πÑ‡∏ü",
            "‡πÄ‡∏õ‡∏¥‡∏î‡∏°‡πà‡∏≤‡∏ô‡∏£‡∏±‡∏ö‡πÅ‡∏™‡∏á‡∏ò‡∏£‡∏£‡∏°‡∏ä‡∏≤‡∏ï‡∏¥‡πÉ‡∏ô‡∏ï‡∏≠‡∏ô‡∏Å‡∏•‡∏≤‡∏á‡∏ß‡∏±‡∏ô"
        ];

        // Init Selects
        const hourSel = document.getElementById('hour-select');
        const minSel = document.getElementById('min-select');
        for(let i=0; i<24; i++) hourSel.add(new Option(i.toString().padStart(2, '0'), i));
        for(let i=0; i<60; i++) minSel.add(new Option(i.toString().padStart(2, '0'), i));
        hourSel.value = 18;

        function toggleMonitor() {
            const btn = document.getElementById('btn-toggle');
            const status = document.getElementById('status-text');
            isMonitoring = !isMonitoring;

            if(isMonitoring) {
                btn.innerText = "‡∏´‡∏¢‡∏∏‡∏î‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏á‡∏≤‡∏ô";
                btn.className = "w-full py-3 bg-red-500 hover:bg-red-600 text-white font-bold rounded-xl transition shadow-lg";
                status.innerHTML = "<span class='status-active'>‚óè ‡∏£‡∏∞‡∏ö‡∏ö‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏ó‡∏≥‡∏á‡∏≤‡∏ô (Active)</span>";
                startTimer();
            } else {
                btn.innerText = "‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏£‡∏∞‡∏ö‡∏ö‡πÄ‡∏ù‡πâ‡∏≤‡∏£‡∏∞‡∏ß‡∏±‡∏á";
                btn.className = "w-full py-3 bg-green-500 hover:bg-green-600 text-white font-bold rounded-xl transition shadow-lg";
                status.innerHTML = "‚óã ‡∏£‡∏∞‡∏ö‡∏ö‡∏´‡∏¢‡∏∏‡∏î‡∏ó‡∏≥‡∏á‡∏≤‡∏ô (Inactive)";
                clearInterval(timer);
            }
        }

        function startTimer() {
            timer = setInterval(() => {
                const now = new Date();
                const h = parseInt(document.getElementById('hour-select').value);
                const m = parseInt(document.getElementById('min-select').value);
                
                if(now.getHours() === h && now.getMinutes() === m && now.getSeconds() === 0) {
                    manualSave(true);
                }
            }, 1000);
        }

        function manualSave(isAuto = false) {
            const limit = parseFloat(document.getElementById('limit-input').value);
            const used = parseFloat(document.getElementById('used-input').value);
            const now = new Date();
            
            const diff = limit - used;
            const money = diff * MIN_UNIT_PRICE;
            const percent = (used / limit) * 100;

            const record = {
                date: now.toLocaleDateString('th-TH'),
                time: now.toLocaleTimeString('th-TH', {hour: '2-digit', minute:'2-digit'}),
                month: now.toLocaleString('th-TH', { month: 'long', year: 'numeric' }),
                limit, used, money, percent
            };

            historyData.push(record);
            showAnalysis(record, isAuto);
        }

        function showAnalysis(data, isAuto) {
            const box = document.getElementById('analysis-box');
            const bar = document.getElementById('bar-fill');
            const text = document.getElementById('analysis-text');
            const tip = document.getElementById('tip-text');

            box.classList.remove('hidden');
            bar.style.width = Math.min(data.percent, 100) + "%";
            
            if(data.percent > 100) bar.className = "progress-bar h-4 rounded-full bg-red-500";
            else if(data.percent > 80) bar.className = "progress-bar h-4 rounded-full bg-yellow-400";
            else bar.className = "progress-bar h-4 rounded-full bg-green-500";

            let resultMsg = data.money >= 0 
                ? `‚úÖ ‡∏õ‡∏£‡∏∞‡∏´‡∏¢‡∏±‡∏î‡πÑ‡∏ü‡πÑ‡∏î‡πâ ${data.money.toFixed(2)} ‡∏ö‡∏≤‡∏ó` 
                : `‚ö†Ô∏è ‡πÉ‡∏ä‡πâ‡πÑ‡∏ü‡πÄ‡∏Å‡∏¥‡∏ô‡πÑ‡∏õ ${Math.abs(data.money).toFixed(2)} ‡∏ö‡∏≤‡∏ó (‡∏ï‡∏¥‡∏î‡∏•‡∏ö)`;
            
            text.innerHTML = `<b>${isAuto ? '[Auto] ' : ''}‡∏™‡∏£‡∏∏‡∏õ:</b> ‡πÉ‡∏ä‡πâ‡πÑ‡∏ü ${data.used} / ${data.limit} ‡∏´‡∏ô‡πà‡∏ß‡∏¢ (${data.percent.toFixed(1)}%) <br> ${resultMsg}`;
            tip.innerText = "üí° ‡πÄ‡∏Ñ‡∏•‡πá‡∏î‡∏•‡∏±‡∏ö: " + tips[Math.floor(Math.random()*tips.length)];
        }

        function toggleHistory() {
            const box = document.getElementById('history-box');
            box.classList.toggle('hidden');
            if(!box.classList.contains('hidden')) renderHistory();
        }

        function renderHistory() {
            const container = document.getElementById('history-content');
            container.innerHTML = "";

            if(historyData.length === 0) {
                container.innerHTML = "<p class='text-center text-gray-400'>‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏õ‡∏£‡∏∞‡∏ß‡∏±‡∏ï‡∏¥‡∏Å‡∏≤‡∏£‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å</p>";
                return;
            }

            // Group by Month
            const groups = {};
            historyData.forEach(item => {
                if(!groups[item.month]) groups[item.month] = [];
                groups[item.month].push(item);
            });

            // Sort months descending (simple logic)
            Object.keys(groups).reverse().forEach(month => {
                const monthDiv = document.createElement('div');
                monthDiv.className = "border-l-4 border-blue-400 pl-4 py-2";
                
                let totalUsed = 0;
                let totalLimit = 0;
                let logsHtml = "";

                groups[month].forEach(log => {
                    totalUsed += log.used;
                    totalLimit += log.limit;
                    const color = log.money >= 0 ? "text-green-600" : "text-red-500";
                    logsHtml += `<p class="text-xs text-gray-500">${log.date} ${log.time} - <span class="${color}">‡πÉ‡∏ä‡πâ ${log.used} ‡∏´‡∏ô‡πà‡∏ß‡∏¢ (${log.money.toFixed(2)} ‡∏ö.)</span></p>`;
                });

                const monthlySaving = (totalLimit - totalUsed) * MIN_UNIT_PRICE;
                const savingColor = monthlySaving >= 0 ? "text-green-600" : "text-red-600";

                monthDiv.innerHTML = `
                    <h3 class="font-bold text-blue-800">${month}</h3>
                    <div class="mb-2">${logsHtml}</div>
                    <p class="text-sm font-semibold">‡πÄ‡∏â‡∏•‡∏µ‡πà‡∏¢‡∏ï‡πà‡∏≠‡∏ß‡∏±‡∏ô: ${(totalUsed/groups[month].length).toFixed(2)} ‡∏´‡∏ô‡πà‡∏ß‡∏¢</p>
                    <p class="text-sm font-bold ${savingColor}">‡∏™‡∏£‡∏∏‡∏õ‡πÄ‡∏î‡∏∑‡∏≠‡∏ô‡∏ô‡∏µ‡πâ: ${monthlySaving.toFixed(2)} ‡∏ö‡∏≤‡∏ó</p>
                `;
                container.appendChild(monthDiv);
            });
        }
    </script>
